{
  "name": "nl.beinformed.bi.layout.general.modularui",
  "version": "1.0.0",
  "description": "Default UI for Be Informed projects based on the modular UI",
  "main": "WEB-INF/server.js",
  "repository": "http://repo.pd.beinformed.nl/svn/product3/trunk/source/nl.beinformed.bi.layout.general.modularui",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "npm run dev:server && npm run dev:client",
    "dev:server": "webpack --env.devserver --env.development",
    "dev:client": "webpack-dev-server --env.devclient --env.development",
    "build:dev": "webpack --env.development",
    "watch:dev": "webpack --env.development --watch",
    "build:prod": "webpack --env.production",
    "watch:prod": "webpack --env.production --watch",
    "test": "cross-env NODE_ENV=test jest",
    "watch:test": "cross-env NODE_ENV=test jest --watch",
    "lint": "npm run lint:js & npm run lint:scss",
    "lint:js": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "lint:scss": "stylelint \"src/**/*.scss\" --fix",
    "preproduction": "npm run prettier && npm run lint",
    "production": "npm run build:prod",
    "prewatch": "npm run test",
    "watch": "npm run watch:dev",
    "build": "npm run production",
    "start": "npm run watch",
    "flow": "flow",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "prettier": "prettier --write \"./{src,flow-decls}/**/*.{js,scss}\"",
    "dev-server": "webpack-dev-server --inline"
  },
  "jest": {
    "rootDir": "./src",
    "testResultsProcessor": "../node_modules/jest-junit",
    "collectCoverage": true,
    "coverageDirectory": "../coverage",
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "verbose": false,
    "automock": false
  },
  "jest-junit": {
    "suiteName": "General Layout Unit Tests",
    "output": "./jest/test-result.xml"
  },
  "browserslist": [
    "Last 1 versions",
    "> 1% in NL",
    "> 1% in GB",
    "> 1% in US"
  ],
